- if prototype.like_user(current_user)
  = button_to prototype_like_path(prototype, like), method: :delete, class: 'btn btn-default', remote: true do
    = image_tag("icon_heart_red.svg")
    Like
    %span.num= prototype.likes_count
- else
  = button_to prototype_likes_path(prototype), method: :post, class: 'btn btn-default', remote: true do
    = image_tag("icon_heart.svg")
    Like
    %span.num= prototype.likes_count
